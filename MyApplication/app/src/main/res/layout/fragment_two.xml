<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FragmentTwo">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            >
            <ImageButton
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_weight="1"
                android:scaleType="fitCenter"
                android:background="#322F2F"
               android:padding="5dp"
                android:src="@drawable/ic_baseline_check2"
                android:id="@+id/subButton1"/>

            <ImageButton
                android:id="@+id/subButton2"

                android:layout_width="0dp"
                android:layout_height="50dp"
                android:src="@drawable/xltucm"
                android:background="#322F2F"
                android:scaleType="fitCenter"
                android:padding="5dp"
                android:layout_weight="1"
                />
            <ImageButton
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:src="@drawable/qkwl"
                android:background="#322F2F"
                android:scaleType="fitCenter"
                android:id="@+id/subButton3"
                android:padding="5dp"

                android:layout_weight="1"/>
            <ImageButton
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:src="@drawable/tlrp"
                android:background="#322F2F"
                android:scaleType="fitCenter"
                android:id="@+id/subButton4"
                android:padding="5dp"

                android:layout_weight="1"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="찜한상품"
                android:id="@+id/subButton11"
                android:fontFamily="@font/gomm"
                android:textColor="#ffffff"
                android:background="#322F2F"
                android:textAlignment="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="셔츠"
                android:id="@+id/subButton22"
                android:fontFamily="@font/gomm"
                android:textColor="#ffffff"
                android:background="#322F2F"
                android:textAlignment="center"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="바지"
                android:fontFamily="@font/gomm"
                android:textColor="#ffffff"
                android:background="#322F2F"
                android:id="@+id/subButton33"
                android:textAlignment="center"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="악세사리"
                android:fontFamily="@font/gomm"
                android:textColor="#ffffff"
                android:background="#322F2F"
                android:id="@+id/subButton44"
                android:textAlignment="center"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/child_fragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="#F4D0D0">
        </LinearLayout>
    </LinearLayout>


</FrameLayout>