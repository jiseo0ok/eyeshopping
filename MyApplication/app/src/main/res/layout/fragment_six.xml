<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FragmentSix">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            >
            <ImageButton
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_weight="1"
                android:scaleType="fitCenter"
                android:background="#322F2F"
                android:padding="5dp"
                android:src="@drawable/face1"
                android:id="@+id/subButton1"/>

            <ImageButton
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:src="@drawable/shop"
                android:background="#322F2F"
                android:scaleType="fitCenter"
                android:id="@+id/subButton2"
                android:padding="5dp"

                android:layout_weight="1"/>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="유저관리"
                android:fontFamily="@font/gomm"
                android:textColor="#ffffff"
                android:background="#322F2F"
                android:id="@+id/subButton11"
                android:textAlignment="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="가게관리"
                android:fontFamily="@font/gomm"
                android:textColor="#ffffff"
                android:background="#322F2F"
                android:id="@+id/subButton22"
                android:textAlignment="center"/>

        </LinearLayout>


        <LinearLayout
            android:id="@+id/child_fragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="#F4D0D0">
        </LinearLayout>
    </LinearLayout>


</FrameLayout>